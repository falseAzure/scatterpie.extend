Package: scatterpie.extend
Type: Package
Title: Addon to the Scatterpie Package
Version: 0.2.0
Authors@R: person("Gabelberger", "Fabian", email =
    "fabian.gabelberger@wifo.ac.at", role = c("cre", "aut"))
